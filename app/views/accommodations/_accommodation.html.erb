<div class="journey-item car" data-lat="<%= accommodation.latitude %>" data-long="<%= accommodation.longitude %>" data-id="<%= accommodation.id %>">
  <div class="journey-item-illustration">
    <%= image_tag("airbnb.png", width: "150", class: "image-radius") %>
  </div>
  <div class="journey-item-main-informations">
    <h3><%= accommodation.name %></h3>
    <h4 class="time">
      <%= accommodation.departure_date.strftime("%b %d, %Y at %H:%M") %>
      <span>â†’</span>
      <%= accommodation.arrival_date.strftime("%b %d, %Y at %H:%M") %>
    </h4>
    <h6><%= accommodation.reservation_number %></h6>
    <p>
      <i class="small material-icons">place</i> <%= accommodation.address %>
    </p>
  </div>
</div>
